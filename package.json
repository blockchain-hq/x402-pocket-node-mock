{
  "name": "@blockchainhq-xyz/n8n-nodes-pocket-mock",
  "version": "1.0.2",
  "description": "n8n node for Solana x402 payment protocol - Return HTTP 402 Payment Required",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "solana",
    "x402",
    "payment",
    "http-402"
  ],
  "license": "MIT",
  "homepage": "https://github.com/blockchain-hq/x402-pocket-node-mock",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockchain-hq/x402-pocket-node-mock.git"
  },
  "author": {
    "name": "Mahima Thacker"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp nodes/SolanaX402/pocket.svg dist/nodes/SolanaX402/ && cp nodes/SolanaX402/SolanaX402.node.json dist/nodes/SolanaX402/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build",
    "test": "npx @n8n/scan-community-package n8n-nodes-pocket-mock"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SolanaX402Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PocketNodeMock/PocketNodeMock.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "n8n-workflow": "*",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "x402-server-sdk": "^1.0.0"
  }
}
